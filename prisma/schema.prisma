generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Jamaah {
  id                Int      @id @default(autoincrement()) 
  namaLengkap       String
  nik               String
  tempatLahir       String
  tanggalLahir      DateTime
  alamat            String
  provinsi          String
  kabKota           String
  kecamatan         String
  kelurahan         String
  jenisKelamin      String
  noPaspor          String
  masaBerlakuPaspor DateTime
  noVisa            String? 
  berlakuVisa       DateTime? 
  paketDipilih      String
  kamarDipilih      String
  lampiranKTP       String
  lampiranKK        String
  lampiranFoto      String
  lampiranPaspor    String
}

